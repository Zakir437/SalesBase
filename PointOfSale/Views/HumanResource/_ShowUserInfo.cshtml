@model PointOfSale.Models.ViewUserList
<h3 style="margin-top:0px!important;">
    Personal Information
    <span class="btn_Dis" id="btn_EditPersonalInfo" data-toggle="tooltip" title="Edit Personal Information"><span><i class="fa fa-pencil"></i></span></span>
</h3><hr />

<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 row" style="margin-bottom:35px">
    <div class="form-horizontal form-widgets col-sm-6">
        <div class="form-group">
            @Html.LabelFor(model => model.FirstName, "User Name", htmlAttributes: new { @class = "control-label col-md-4 col-sm-5 col-xs-12" })
            <div class="col-md-6 col-sm-7 col-xs-8 dis_info" style="padding-top:7px">
                @if (Model.Title != null)
                {
                    @Html.DisplayFor(model => model.Title, new { htmlAttributes = new { @class = "form-control k-textbox" } })
                }
                @Html.DisplayFor(model => model.FirstName, new { htmlAttributes = new { @class = "form-control k-textbox" } })
                @Html.DisplayFor(model => model.MiddleName, new { htmlAttributes = new { @class = "form-control k-textbox" } })
                @Html.DisplayFor(model => model.LastName, new { htmlAttributes = new { @class = "form-control k-textbox" } })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.DateOfBirth, "Date of Birth", htmlAttributes: new { @class = "control-label col-md-4 col-sm-5 col-xs-12" })
            <div class="col-md-6 col-sm-7 col-xs-8 dis_info" style="padding-top:7px">
                @(((DateTime)Model.DateOfBirth).ToString("MMMM dd,yyyy"))
            </div>
        </div>
        <div class="form-group">
            @Html.Label("Gender", htmlAttributes: new { @class = "control-label col-md-4 col-sm-5 col-xs-12" })
            <div class="col-md-6 col-sm-7 col-xs-8 dis_info" style="padding-top:7px">
                @if (Model.Gender == 1)
                {
                    <span>Male</span>
                }
                else if (Model.Gender == 2)
                {
                    <span>Female</span>
                }
            </div>
        </div>
    </div>
    <div class="form-horizontal form-widgets col-sm-6">
        <div class="form-group">
            @Html.LabelFor(model => model.Nationalitiy, "National", htmlAttributes: new { @class = "control-label col-md-4 col-sm-5 col-xs-12" })
            <div class="col-md-6 col-sm-7 col-xs-8 dis_info" style="padding-top:7px">
                @Html.DisplayFor(model => model.Nationalitiy, new { htmlAttributes = new { @class = "form-control k-textbox" } })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Religion, "Religion", htmlAttributes: new { @class = "control-label col-md-4 col-sm-5 col-xs-12" })
            <div class="col-md-6 col-sm-7 col-xs-8 dis_info" style="padding-top:7px">
                @Html.DisplayFor(model => model.Religion, new { htmlAttributes = new { @class = "form-control k-textbox" } })
            </div>
        </div>
        @if (Model.NationalId != null)
        {
            <div class="form-group">
                @Html.LabelFor(model => model.NationalId, "National Id No", htmlAttributes: new { @class = "control-label col-md-4 col-sm-5 col-xs-12" })
                <div class="col-md-6 col-sm-7 col-xs-8 dis_info" style="padding-top:7px">
                    @if (Model.NationalIdFontImg != null || Model.NationalIdBackImg != null)
                    {
                        <a href="javascript:;" id="ShowNationalId">@Html.DisplayFor(model => model.NationalId, new { htmlAttributes = new { @class = "form-control k-textbox" } })</a>
                    }
                    else
                    {
                        @Html.DisplayFor(model => model.NationalId, new { htmlAttributes = new { @class = "form-control k-textbox" } })
                    }
                </div>
            </div>
        }
    </div>
</div>
<h3>
    Contact Information
    <span class="btn_Dis" id="btn_EditContactInfo"><span><i class="fa fa-pencil" data-toggle="tooltip" title="Edit Contact Information"></i></span></span>
</h3><hr />
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 row" style="margin-bottom:35px">
    <div class="form-horizontal form-widgets col-sm-6">
        <div class="form-group">
            @Html.LabelFor(model => model.EmailAddress, "E-mail", htmlAttributes: new { @class = "control-label col-md-4 col-sm-5 col-xs-12" })
            <div class="col-md-6 col-sm-7 col-xs-8 dis_info" style="padding-top:7px">
                @Html.DisplayFor(model => model.EmailAddress, new { htmlAttributes = new { @class = "form-control k-textbox" } })
            </div>
        </div>
       
    </div>
    <div class="form-horizontal form-widgets col-sm-6">
        @if (Model.MobileNo != null)
         {
            <div class="form-group">
                @Html.LabelFor(model => model.MobileNo, "Mobile Number", htmlAttributes: new { @class = "control-label col-md-4 col-sm-5 col-xs-12" })
                <div class="col-md-6 col-sm-7 col-xs-8 dis_info" style="padding-top:7px">
                    @Html.DisplayFor(model => model.MobileNo, new { htmlAttributes = new { @class = "form-control k-textbox" } })
                </div>
            </div>
         }
    </div>
</div>
