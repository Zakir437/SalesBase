                {
                    <div class="form-group">
                        @Html.LabelFor(model => model.PreDivision, "Division", htmlAttributes: new { @class = "control-label col-md-4 col-sm-5 col-xs-12" })
                        <div class="col-md-6 col-sm-7 col-xs-8 dis_info" style="padding-top:7px">
                            @Html.DisplayFor(model => model.PreDivision, new { htmlAttributes = new { @class = "form-control k-textbox" } })
                        </div>
                    </div>
                }
