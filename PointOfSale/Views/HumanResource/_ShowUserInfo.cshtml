            </div>
         }
    </div>
</div>
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 row" style="margin-bottom:35px">
    @if (Model.SamePresentAddress == true)
    {
        <div class="form-horizontal form-widgets col-sm-12">
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <label class="control-label">Permanent address is also used as present address</label>
                </div>
            </div>
        </div>
    }
</div>
@if (!Model.SamePresentAddress)
 {
    <h3>
        Present Address
        <span class="btn_Dis" id="btn_EditPresentAddress" data-toggle="tooltip" title="Edit Present Address"><span><i class="fa fa-pencil"></i></span></span>
    </h3><hr />
