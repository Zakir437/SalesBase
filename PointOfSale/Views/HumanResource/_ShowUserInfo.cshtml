                    <div class="col-md-6 col-sm-7 col-xs-8 dis_info" style="padding-top:7px">
                        @Html.DisplayFor(model => model.PreCountry, new { htmlAttributes = new { @class = "form-control k-textbox" } })
                    </div>
                </div>
