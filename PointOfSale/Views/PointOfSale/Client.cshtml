<style>
    .k-edit-form-container .k-button{
        margin:0;
    }
</style>
<div class="k-edit-form-container col-lg-11 col-md-10 col-sm-11 col-xs-10" style="margin:16px;">
    <div>
        
    </div>
    <div id="divClientList"></div>
    <div class="k-edit-buttons k-state-default">
        <button type="button" class="k-button k-button-icontext k-primary k-grid-update" data-id="" data-name="" data-iscredit="" data-prevdue="0" data-creditlimit="0" id="btnSetClient"><span class="k-icon k-update"></span>Ok</button>
        @*<button type="reset" class="k-button k-button-icontext k-grid-update" id="btnRefresh"><span class="k-icon k-i-refresh"></span>Refresh</button>*@
        <button type="button" class="k-button k-button-icontext k-grid-cancel" id="btnClientCancel"><span class="k-icon k-cancel"></span>Cancel</button>
    </div>
</div>
<script>
    $(document).ready(function () {
        $("#btnSetClient").prop("disabled", true);
        $("#divClientList").empty();
        $("#divClientList").load('@Url.Action("ClientList", "PointOfSale")');
    });
    $("#btnClientCancel").click(function () {
        $(this).closest(".k-window-content").data("kendoWindow").close();
    });
    //var bcashValidator = $("#divClientList").kendoValidator().data("kendoValidator");
</script>
