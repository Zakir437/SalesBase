<div class="panel panel-info">
    <div class="panel-heading" style="padding-bottom:46px;">
        <div class="pull-left">
            <h4 class="uppercase">Sales Order Information <i class="fa fa-chevron-right"></i> Payment Transaction</h4>
        </div>
        <div class="pull-right">
            <a class="btn btn-default" id="btnBackInPaymentTrans"><i class="fa fa-arrow-left"></i> Back</a>
        </div>
    </div>
    <div class="panel-body">
        <div id="divPaymentHistoryList"></div>
    </div> 
</div>
<script>
    $(document).ready(function () {
        $("#divPaymentHistoryList").append('<div class="tiny_loading"></div>');
        $("#divPaymentHistoryList").load('@Url.Action("PaymentHistoryList", "Sales")?orderId=' + '@ViewBag.OrderId');
    })
</script>
